    <PackageReference Include="System.Drawing.Common" Version="8.0.6" />
    <PackageReference Include="TorchSharp-cuda-windows" Version="0.102.6" />
    <PackageReference Include="TorchVision" Version="0.102.6" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="outputs\" />
